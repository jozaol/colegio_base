<?xml version="1.0"?>
<odoo>
  <record id="module_notas_category" model="ir.module.category">
    <field name="name">Notas</field>
    </record>
      <!-- Notas User Group -->
  <record id="notas_group_user" model="res.groups">
    <field name="name">User</field>
    <field name="category_id"
           ref="module_notas_category"/>
    <field name="implied_ids"
           eval="[(4, ref('base.group_user'))]"/>
  </record> 
  <!--Notas Manager Group-->
  <record id="notas_group_manager" model="res.groups">
    <field name="name">Manager</field>
    <field name="category_id"
           ref="module_notas_category"/>
    <field name="implied_ids"
           eval="[(4, ref('notas_group_user'))]"/>
    <field name="users"
           eval="[(4, ref('base.user_root')),
                  (4, ref('base.user_admin'))]"/>
 </record>
</odoo>